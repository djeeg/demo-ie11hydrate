{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmit": false,
    "noEmitOnError": false,
    "pretty": false,
    "allowUnreachableCode": true,
    "listFiles": false,
    "experimentalDecorators": false,
    "removeComments": false,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "noLib": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "baseUrl": "node_modules"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "output"
  ]
}
